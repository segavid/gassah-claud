{
  "version": 2,
  "rewrites": [
    {
      "source": "/wp-content/:path*",
      "destination": "https://gesseh.net/wp-content/:path*"
    },
    {
      "source": "/wp-includes/:path*",
      "destination": "https://gesseh.net/wp-includes/:path*"
    },
    {
      "source": "/:path*.(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot|webp|mp4|webm|ogg|mp3|wav|flac|aac)",
      "destination": "https://gesseh.net/:path*.$1"
    },
    {
      "source": "/robots.txt",
      "destination": "https://gesseh.net/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "https://gesseh.net/sitemap.xml"
    },
    {
      "source": "/sitemap:path*.xml",
      "destination": "https://gesseh.net/sitemap:path*.xml"
    },
    {
      "source": "/:path*",
      "destination": "/api/index.py"
    }
  ],
  "headers": [
    {
      "source": "/wp-content/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/wp-includes/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
