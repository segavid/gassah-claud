{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/wp-content/(.*)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "https://gesseh.com/wp-content/$1"
    },
    {
      "src": "/wp-includes/(.*)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "https://gesseh.com/wp-includes/$1"
    },
    {
      "src": "/(.*\\.(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot|webp|xml|txt))",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "https://gesseh.com/$1"
    },
    {
      "src": "/robots.txt",
      "dest": "https://gesseh.com/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "https://gesseh.com/sitemap.xml"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
