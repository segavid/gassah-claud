{
  "rewrites": [
    {
      "source": "/wp-content/(.*)",
      "destination": "https://gesseh.net/wp-content/$1"
    },
    {
      "source": "/wp-includes/(.*)",
      "destination": "https://gesseh.net/wp-includes/$1"
    },
    {
      "source": "/(.*\\.(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot|webp|mp4|webm))",
      "destination": "https://gesseh.net/$1"
    },
    {
      "source": "/robots.txt",
      "destination": "https://gesseh.net/robots.txt"
    },
    {
      "source": "/sitemap(.*).xml",
      "destination": "https://gesseh.net/sitemap$1.xml"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
